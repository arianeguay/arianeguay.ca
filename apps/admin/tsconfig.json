      "@arianeguay/ui": ["../../libs/ui/src/index.ts"],
      "@arianeguay/utils": ["../../libs/utils/src/index.ts"]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ]
}
